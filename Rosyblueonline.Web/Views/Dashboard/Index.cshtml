@model List<OrderListView>
@{
    ViewBag.Title = "Home";
    string curRoleId = "";
    Rosyblueonline.Models.TokenLogModel objToken = Session["Token"] as Rosyblueonline.Models.TokenLogModel;
    UserMenuAccessModel objMenuAccess = Session["MenuAccess"] as UserMenuAccessModel;
    if (objToken != null)
    {
        curRoleId = objToken.RoleID.ToString();
    }

}
@section header{
}
<div class="content-wrapper">
    <section class="content">
        <div id="uiPanel1"></div>
        <section class="container-fluid">
            <!--<div class="panel-stonebox">
                <div class="heading-title">
                    <h2>@Resource.Dashboard</h2>
                </div>
            </div-->

            <div class="row">
                <div class="col-lg-8 col-md-8">
                    <div class="dashtop-box">
                        @if (curRoleId == "2" || curRoleId == "8" || (curRoleId == "9" && (objToken.loginID == 11749 || objToken.loginID == 3379)))
                        {
                            if (objMenuAccess.AddCustomer == 1)
                            {
                                <div class="dash-box">
                                    <div class="dash-head">@Resource.Customer_Update</div>
                                    <div class="dash-inner">
                                        <div class="row">
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <div class="dashboard-icon">
                                                    <a href="/User" class="icon-label">
                                                        <img src="~/content/img/icon_7.jpg">
                                                        <span class="icon-label">@Resource.List_of_Customers</span>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        }

                        @if (objMenuAccess.Inventory == 1)
                        {
                            <div class="dash-box">
                                <div class="dash-head">@Resource.Inventory</div>
                                <div class="dash-inner">
                                    <div class="row">
                                        @if (objMenuAccess.AllMemo == 1)
                                        {
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <div class="dashboard-icon">
                                                    <a href="/Memo" class="icon-label">
                                                        <img src="~/content/img/icon_2.jpg">
                                                        <span class="icon-label">@Resource.Inventory_on_Memo</span>
                                                    </a>
                                                </div>
                                            </div>
                                        }
                                        @*<div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                            <a href="#" class="dash-link"></a>
                            <div class="dashboard-icon">
                                <a href="@Url.Action("Index", "DownloadInventory")" class="icon-label">
                                    <img src="~/content/img/icon_11.jpg">
                                    <span class="icon-label">Inventory Download</span>
                                </a>
                            </div>
                        </div>*@
                                        @if (objMenuAccess.InventoryDownload == 1)
                                        {
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <a href="#" class="dash-link"></a>
                                                <div class="dashboard-icon">
                                                    <a href="/Inventory/downloads" class="icon-label">
                                                        <img src="~/content/img/icon_11.jpg">
                                                        <span class="icon-label">@Resource.Inventory_Download</span>
                                                    </a>
                                                </div>
                                            </div>
                                        }
                                        @if (objMenuAccess.InventoryUpload == 1)
                                        {
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <a href="#" class="dash-link"></a>
                                                <div class="dashboard-icon">
                                                    <a href="/Inventory/upload" class="icon-label">
                                                        <img src="~/content/img/icon_15.jpg">
                                                        <span class="icon-label">@Resource.Inventory_Upload</span>
                                                    </a>
                                                </div>
                                            </div>
                                        }
                                        @if (objMenuAccess.UploadHistory == 1)
                                        {

                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <a href="#" class="dash-link"></a>
                                                <div class="dashboard-icon">
                                                    <a href="/Inventory/UploadHistory" class="icon-label">
                                                        <img src="~/content/img/icon_15.jpg">
                                                        <span class="icon-label">@Resource.Upload_History</span>
                                                    </a>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        }

                        @if (objMenuAccess.Search == 1)
                        {
                            <div class="dash-box">
                                <div class="dash-head">@Resource.Search</div>
                                <div class="dash-inner">
                                    <div class="row">
                                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                            <a href="#" class="dash-link"></a>
                                            <div class="dashboard-icon">

                                                <a href="/Inventory/SpecificSearch" class="dash-link">
                                                    <img src="~/content/img/icon_5.jpg">
                                                    <span class="icon-label">@Resource.Specfic_search</span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (curRoleId != "3" && curRoleId != "4")
                        {

                            if (objMenuAccess.RFID == 1)
                            {
                                <div class="dash-box">
                                    <div class="dash-head">@Resource.RFID</div>
                                    <div class="dash-inner">
                                        <div class="row">
                                            @if (objMenuAccess.RFIDSearch == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="http://86.96.199.117:91" target="_blank" class="dash-link">
                                                            <img src="~/content/img/icon_4.jpg">
                                                            <span class="icon-label">@Resource.RFID_Search</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.RFIModify == 1)
                                            {

                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/RFID/Update" class="dash-link">
                                                            <img src="~/content/img/icon_15.jpg">
                                                            <span class="icon-label">@Resource.RFID_Modify</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.RFIDUploadHistory == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/RFID/Uploadhistory" class="dash-link">
                                                            <img src="~/content/img/icon_15.jpg">
                                                            <span class="icon-label">@Resource.Upload_History</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.RFIDStockTally == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="http://86.96.199.117:91" target="_blank" class="dash-link">

                                                            <img src="~/content/img/icon_6.jpg">
                                                            <span class="icon-label">@Resource.RFID_Stock_Tally</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.RFIDMaster == 1)
                                            {

                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/RFID/RFIDMaster" class="dash-link">
                                                            <img src="~/content/img/icon_15.jpg">
                                                            <span class="icon-label">@Resource.RFID_Master</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.RFIDHistory == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/RFID/History" class="dash-link">
                                                            <img src="~/content/img/icon_15.jpg">
                                                            <span class="icon-label">@Resource.RFID_History</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            }

                        }
                        @if (curRoleId != "3" && curRoleId != "9" && curRoleId != "4")
                        {
                           if (objMenuAccess.StoneStatus == 1)
                            {
                                <div class="dash-box">
                                    <div class="dash-head">@Resource.Report</div>
                                    <div class="dash-inner">
                                        <div class="row">

                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                <div class="dashboard-icon">
                                                    <a href="/Inventory/StoneStatus" class="dash-link">
                                                        <img src="~/content/img/icon_16.jpg">
                                                        <span class="icon-label">@Resource.Stone_Status</span>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                        @if (curRoleId == "2")
                        {
                            if (objMenuAccess.Marketing == 1)
                            {
                                <div class="dash-box">
                                    <div class="dash-head">@Resource.Marketing</div>
                                    <div class="dash-inner">
                                        <div class="row">
                                            @if (objMenuAccess.BlueNile == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/Marketing/BlueNile" class="dash-link">
                                                            <img src="~/content/img/icon_9.jpg">
                                                            <span class="icon-label">@Resource.BlueNile_Disc</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.JamesAllen == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/Marketing/JamesAllen" class="dash-link">
                                                            <img src="~/content/img/icon_9.jpg">
                                                            <span class="icon-label">@Resource.JamesAllen_Disc</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                            @if (objMenuAccess.JamesAllenHK == 1)
                                            {
                                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4 hvr-grow">
                                                    <a href="#" class="dash-link"></a>
                                                    <div class="dashboard-icon">
                                                        <a href="/Marketing/JamesAllenHK" class="dash-link">
                                                            <img src="~/content/img/icon_9.jpg">
                                                            <span class="icon-label">@Resource.JamesAllen_HK_Disc</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>

                            }
                        }


                    </div>
                </div>
                <div class="col-lg-4 col-md-4">
                    @if (curRoleId == "2" || curRoleId == "9")
                    {
                       if (objMenuAccess.AllOrder == 1)
                        {
                            <div class="dash-box list-view">
                                <ul class="nav nav-tabs">
                                    <li class="active"><a data-toggle="tab" href="o_pendingorderdetail">@Resource.Pending_Order_Report</a></li>
                                    <li><a href="/Order/List">@Resource.View_Order_Details</a></li>
                                </ul>
                                <div class="table-box">
                                    <div class="tab-content">
                                        <div id="o_pendingorderdetail" class="tab-pane fade in active">
                                            <table id="pendingOrder" class="table table-striped table-fixed dataTable no-footer tabledash">
                                                <tr role="row">
                                                    <th class="sorting_asc">
                                                        @Html.Label("Order No")
                                                    </th>
                                                    <th class="sorting">
                                                        @Html.Label("Order Date")
                                                    </th>
                                                    <th class="sorting">
                                                        @Html.Label("Amount (US$)")
                                                    </th>
                                                </tr>

                                                @foreach (var item in Model)
                                                {
                                                    <tr>
                                                        <td>
                                                            @item.orderDetailsId
                                                        </td>
                                                        <td>
                                                            @item.orderCreatedOn.ToString("dd-MM-yyyy")
                                                        </td>
                                                        <td>
                                                            @item.orderPayableAmount
                                                        </td>
                                                    </tr>
                                                }

                                            </table>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        }
                    }

                    <div id="ShoppingCartTab" class="panel panel-default hide">
                        <div class="panel-heading">@Resource.Shopping_Cart</div>
                        <div class="panel-body pending-table">
                            <table id="ShoppingCartSummaryDataTable" class="table customers-table hide">
                                <thead style="font-weight: bold; border-top:1px solid #969595;">
                                    <tr role="row" class="table-head">
                                        <th class="cust-table">
                                            @Html.Label("Lot Number")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Shape")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Size")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Clarity")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Color")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Cut")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Pol")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Sym")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Rap($)")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Disc(%)")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Price($)/ct")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Amount($)")
                                        </th>

                                        <th class="cust-table">
                                            @Html.Label("Name")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Location")
                                        </th>
                                        <th class="cust-table">
                                            @Html.Label("Status")
                                        </th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>


            </div>

        </section>
    </section>
</div>

@section script{
}
