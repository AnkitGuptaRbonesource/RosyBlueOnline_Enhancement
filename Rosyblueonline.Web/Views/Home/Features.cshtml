@{
    ViewBag.Title = "Features";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="body">
    <section class="content-wrapper main-content clear-fix">
        <form method="post">
            <div class="banner-div">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12"><img src="~/Content/img/banner.jpg" width="100%;"></div>
                    </div>
                </div>
            </div>
            <div class="welcome-div">
                <div class="container">
                    <div class="welcome-inner">
                        <div class="welcome_head">@Resource.Rosy_Blue_Online_Features</div>
                        <p class="welcome_para">
                            <strong>@Resource.Rosy_Blue_Online</strong> @Resource.is_our_latest
                        </p>
                        <p class="welcome_para">@Resource.The_site_allows_clients_to_browse</p>

                        <p class="welcome_para">@Resource.The_site_offers_stones_certified</p>

                        <p class="welcome_para">@Resource.Our_innovative</p>
                    </div>
                </div>
            </div>
        </form>
    </section>
</div>

