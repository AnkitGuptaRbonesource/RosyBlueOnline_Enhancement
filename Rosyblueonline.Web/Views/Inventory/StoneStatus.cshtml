@{
    ViewBag.Title = "StoneStatus";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section header{
    @Styles.Render("~/bundles/DataTable/Style")
}
<div class="content-wrapper">
    <div id="uiPanel1"></div>
    <section class="content">
        <div class="row">
            <div class="col-lg-12">
                <form id="frmStoneStatus">

                    <div class="panel panel-primary proshad">
                        <div class="panel-heading">
                            Stone Status
                        </div>
                        <div class="panel-body">
                            <div class="customers-info">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <label class="radio-inline">
                                                <input type="radio" name="optradio" id="rbtnLotNo" value="LOTNO" checked><label for="rbtnLotNo" class="controll-label">Lot No</label>
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="optradio" id="rbtnCertNo" value="CERTNO"><label for="rbtnCertNo" class="controll-label">Cert. No</label>
                                            </label>
                                        </div>
                                        <div class="col-sm-1"><label for="fuName" class="control-label">Select File</label></div>
                                        <div class="col-sm-2">
                                            <input class="form-control" id="fuName" name="Name" type="file">
                                        </div>
                                        <div class="col-sm-1"><label for="fuName" class="control-label">Lot/Cret. No</label></div>
                                        <div class="col-sm-3">
                                            <input class="form-control" id="txtLotNo" name="LotNoName" type="text">
                                        </div>
                                        <div class="col-sm-1">
                                            <button class="btn btn-primary pull-right" id="btnSubmit">Submit</button>
                                        </div>
                                        <div class="col-sm-1">
                                            <button class="btn btn-primary pull-right" id="btnExcel">Excel</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="/Inventory/ExportToExcelInventoryDownload?filename=StoneStatusExport" id="ancDownloadExcel" target="_blank" style="display:none;">Download Excel</a>
                </form>
            </div>
            <div class="col-md-12">
                <div class="panel panel-primary">

                    <div class="panel-body paneltable">
                        <table id="SearchTablePost" class="table table-fixed dataTable no-footer" role="grid" aria-describedby="SearchTablePost_info">
                            <thead class="bordertp">
                                <tr role="row" class="table-head">
                                    <th class="tbnew">
                                        <label for="Stock">Stock</label>
                                    </th>
                                    <th class="tbnew Stock">
                                        <label for="Stock">Status</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Certificate_">Certificate#</label>
                                    </th>
                                    <th class="tbnew dt-V360">
