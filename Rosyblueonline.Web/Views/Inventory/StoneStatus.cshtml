@{
    ViewBag.Title = "StoneStatus";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section header{
    @Styles.Render("~/bundles/DataTable/Style")
}
<div class="content-wrapper">
    <div id="uiPanel1"></div>
    <section class="content">
        <div class="row">
            <div class="col-lg-12 col-sm-12 col-xs-12">
                <div class="memo-title status-title">
                    <h2>@Resource.Stone_Status <div class="inb" id="StoneCounts"></div> </h2>
                </div>
            </div>
            <div class="col-lg-12">
                <form id="frmStoneStatus">

                    <div class="dash-box">
                        <div class="dash-inner">
                            <div class="row">
                                <div class="col-lg-12 col-sm-12 col-xs-12">
                                    <div class="status-boxes">
                                        <div class="status-box">
                                            <label class="radio-inline">
                                                <input type="radio" name="optradio" id="rbtnLotNo" value="LOTNO" checked><label for="rbtnLotNo" class="controll-label">@Resource.Lot_No</label>
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="optradio" id="rbtnCertNo" value="CERTNO"><label for="rbtnCertNo" class="controll-label">@Resource.Cert_No</label>
                                            </label>
                                        </div>
                                        <div class="status-box">
                                            <label for="fuName" class="control-label">@Resource.Select_file</label>
                                            <input class="form-control" id="fuName" name="Name" type="file">
                                        </div>
                                        <div class="status-box">
                                            <label for="fuName" class="control-label">@Resource.Lot_Cret_No</label>
                                            <input class="form-control" id="txtLotNo" name="LotNoName" type="text">
                                        </div>
                                        <div class="status-box2">
                                            <div class="status-btn">
                                                <button class="btn btn-primary pull-right" id="btnSubmit">@Resource.Submit</button>
                                                <button class="btn btn-primary pull-right" id="btnExcel">@Resource.Excel</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="/Inventory/ExportToExcelInventoryDownload?filename=StoneStatusExport" id="ancDownloadExcel" target="_blank" style="display:none;">@Resource.Download_Excel</a>
                </form>
            </div>
            <div class="col-md-12">
                <div class="dash-box">
                    <div class="table-box">
                        <table id="SearchTablePost" class="table table-fixed dataTable no-footer" role="grid" aria-describedby="SearchTablePost_info">
                            <thead class="bordertp">
                                <tr role="row" class="table-head">
                                    <th class="tbnew">
                                        <label for="Stock">@Resource.Stock</label>
                                    </th>
                                    <th class="tbnew Stock">
                                        <label for="Stock">@Resource.Status</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Certificate_">@Resource.CertificateId</label>
                                    </th>
                                    <th class="tbnew dt-V360">
                                        <label for="V360">@Resource.V360</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Shape">@Resource.Shape</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Weight">@Resource.Weight</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Color">@Resource.Color</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Clarity">@Resource.Clarity</label>
                                    </th>
                                    <th class="tbnew">
                                        <label for="Diamentions">@Resource.Diamentions</label>
