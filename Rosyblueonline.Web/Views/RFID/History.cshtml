@{
    ViewBag.Title = "RFID History";
}
@section header{
    @Styles.Render("~/bundles/DataTable/Style")
}
<div class="content-wrapper">
    <section class="content">
        <div id="uiPanel1"></div>
        <div class="row">
            <div class="col-lg-12">
                <section class="panel panel-primary">
                    <div class="panel-heading">
                        @Resource.RFID_History
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table id="tblRFID" class="table table-condensed">
                                    <thead>
                                        <tr role="row" class="table-head">
                                            <th class="cust-table">
                                                @Resource.RFID_No
                                            </th>
                                            <th class="cust-table">
                                                @Resource.Certificate_No
                                            </th>
                                            <th class="cust-table">
                                                @Resource.Created_On
                                            </th>
                                            <th class="cust-table">
                                                @Resource.Created_By
                                            </th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>

        </div>

    </section>
</div>
@section script{
    @Scripts.Render("~/bundles/DataTable/Script")
    <script type="text/javascript" src="~/Scripts/custom/rbDataTable.js"></script>
    <script src="~/Scripts/custom/Controller/CtrlRFIDHistory.js"></script>
}



