@model int
@{
    ViewBag.Title = "Tally Memo";
}
@section header{
    @Styles.Render("~/bundles/DataTable/Style")
}
<div class="content-wrapper">
    <div id="uiPanel1"></div>
    <section class="content">
        <div class="row">
            <div class="col-lg-5 col-md-5">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        RFID Memo Stock Tally
                    </div>
                    <div class="panel-body">
                        <div class="">
                            <div class="customers-info form-horizontal">
                                <div class="rfid-mch">
                                    <div class="form-group">
                                        <label class="col-sm-4">Machine</label>
                                        <div class="col-sm-8">
                                            <select id="ddlipaddress" class="form-control">
                                                <option value="192.168.20.121">RFID_1</option>
                                                <option value="192.168.20.122">RFID_2</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="rfid-number">
                                    <div class="form-group">
                                        <label class="col-sm-4">RFId Number</label>
                                        <div class="col-sm-8">
                                            <textarea id="list" name="Command" class="form-control" disabled></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="total-rfid">
                                    <div class="form-group">
                                        <label class="col-sm-4">Total RFID Count</label>
                                        <div class="col-sm-8">
                                            <div id="count"></div>
                                            <div id="rfidtotalCount"></div>
                                            <div id="summary"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="total-rfid">
                                    <div class="form-group">
                                        <label class="col-sm-4">Recent RFID Count</label>
                                        <div class="col-sm-8">
                                            <div id="notify"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="total-rfid">
                                    <div class="form-group">
                                        <label class="col-sm-4">Continue mode</label>
                                        <div class="col-sm-8">
                                            <div class="checkbox">
                                                <label><input type="checkbox" value="" id="chkContinueMode"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-6">
                                        <button id="btnScanRF" class="btn btn-primary pull-right">Scan</button>
                                    </div>
                                    <div class="col-sm-6">
                                        <button id="btnReset" name="command" value="reset" class="btn btn-warning">Reset</button>
                                        <input id="hfMemoID" value="@Model.ToString()" type="hidden" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="customers-list-table">
        <div class="col-md-12">
            <div class="panel panel-primary">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-6 pull-right" id="Divbtnexport">
                            <button id="btnexport" class="btn btn-primary pull-right">ExportToExcel</button>
                        </div>
                    </div>
                </div>
                
                <div class="panel-body paneltable">
                    <table id="SearchTablePost" class="table table-fixed dataTable no-footer" role="grid" aria-describedby="SearchTablePost_info">
                        <thead class="bordertp">
                            <tr role="row" class="table-head">
                                <th class="tbnew">
                                    <label for="Stock">Stock</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Certificate_">Certificate#</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Shape">Rfid</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Shape">Shape</label>
                                </th>
                                <th class="tbnew dt-weight">
                                    <label for="Weight">Weight</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Color">Color</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Clarity">Clarity</label>
                                </th>
                                <th class="tbnew">
                                    <label for="Cut">Cut</label>
                                </th>
                                <th class="tbnew">
                                    <label for="MO">Memo/Order</label>
                                </th>
                                <th class="tbnew">
                                    <label for="MS">Memo Status</label>
                                </th>
                                <th class="tbnew dt-status">
                                    <label for="MS">Stock Status</label>
                                </th>
                            </tr>
                        </thead>

                    </table>

                </div>
            </div>
        </div>
    </div>
</div>
@section script{
    @Scripts.Render("~/bundles/DataTable/Script")
    <script type="text/javascript" src="//cdn.rawgit.com/rainabba/jquery-table2excel/1.1.0/dist/jquery.table2excel.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/fixedcolumns/3.3.0/js/dataTables.fixedColumns.min.js"></script>
    <script type="text/javascript" src="~/Scripts/custom/rbDataTable.js"></script>
    <script type="text/javascript" src="~/Scripts/spacecode.js"></script>
    <script type="text/javascript" src="~/Scripts/custom/Service/ServiceMethods.js"></script>
    <script type="text/javascript" src="~/Scripts/custom/Controller/CtrlRFIDStockTallyMemo.js"></script>
}



