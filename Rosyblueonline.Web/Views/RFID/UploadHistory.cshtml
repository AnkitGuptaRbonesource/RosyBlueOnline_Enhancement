@{
    ViewBag.Title = "RFID Upload History";
}
@section header{
    @Styles.Render("~/bundles/DataTable/Style")
}
<div class="content-wrapper">
    @*<div id="uiPanel1"></div>*@
    <section class="content">
        <div id="uiPanel1"></div>
        <div class="container-fluid">
            <div class="dash-box mt-10">
                <div class="dash-head">
                    Upload History
                </div>
                <div class="table-box list-view">
                    <table id="tblUploadHistory" class="table table-fixed dataTable no-footer" role="grid" aria-describedby="SearchTablePost_info">
                        <thead class="bordertp">
                            <tr role="row" class="table-head">
                                <th class="tbnew">
                                    <label>@Resource.File_ID</label>
                                </th>
                                <th class="tbnew">
                                    <label>@Resource.File_Type</label>
                                </th>
                                @*<th class="tbnew">
                            <label>Before Change</label>
                        </th>*@
                                <th class="tbnew">
                                    <label>@Resource.File_Name</label>
                                </th>
                                <th class="tbnew">
                                    <label>@Resource.Uploaded_Date</label>
                                </th>
                                <th class="tbnew">
                                    <label>@Resource.Valid</label>
                                </th>
                                <th class="tbnew">
                                    <label>@Resource.Invalid</label>
                                </th>
                            </tr>
                        </thead>

                    </table>

                </div>
            </div>
        </div>
    </section>
</div>
@section script{
    @Scripts.Render("~/bundles/DataTable/Script")
    <script type="text/javascript" src="~/Scripts/custom/rbDataTable.js"></script>
    <script type="text/javascript" src="~/Scripts/custom/Controller/CtrlInventoryUploadHistoryRfid.js"></script>
}


