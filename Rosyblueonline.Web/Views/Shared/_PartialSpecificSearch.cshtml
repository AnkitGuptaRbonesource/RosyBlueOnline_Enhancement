                            <div>
                                <div class="tooltip fade bottom in" role="tooltip" name="shap" id="pear_shapebt_div">
                                    <div class="tooltip-arrow"></div>
                                    <div class="tooltip-inner">@Resource.Pear</div>
                                </div>
                            </div>
                        </div>
                        <div class="diamond-box">
                            <input class="radiant_shape" id="radiant_shapebt" name="shap" value="9" type="button" data-toggle="tooltip" data-placement="bottom" title="">
                            <div>
                                <div class="tooltip fade bottom in" role="tooltip" name="shap" id="radiant_shapebt_div">
                                    <div class="tooltip-arrow"></div>
                                    <div class="tooltip-inner">@Resource.Radiant</div>
                                </div>
                            </div>
                        </div>
                        <div class="diamond-box">
                            <input class="heart_shape" id="heart_shapebt" name="shap" value="4" type="button" data-toggle="tooltip" data-placement="bottom" title="">
                            <div>
                                <div class="tooltip fade bottom in" role="tooltip" name="shap" id="heart_shapebt_div">
                                    <div class="tooltip-arrow"></div>
                                    <div class="tooltip-inner">@Resource.Heart</div>
                                </div>
                            </div>
                        </div>
                        <div class="diamond-box">
                            <input class="marquise_shape" id="marquise_shapebt" name="shap" value="5" type="button" data-toggle="tooltip" data-placement="bottom" title="">
                            <div>
                                <div class="tooltip fade bottom in" role="tooltip" name="shap" id="marquise_shapebt_div">
                                    <div class="tooltip-arrow"></div>
                                    <div class="tooltip-inner">@Resource.Marquise</div>
                                </div>
