                                    <div class="tooltip-inner">@Resource.Oval</div>
                                </div>
                            </div>
                        </div>
                        <div class="diamond-box">
                            <input class="other_shape" id="other_shapebt" name="shap" value="18" type="button" data-toggle="tooltip" data-placement="bottom" title="">
                            <div>
                                <div class="tooltip fade bottom in" role="tooltip" name="shap" id="other_shapebt_div">
                                    <div class="tooltip-arrow"></div>
                                    <div class="tooltip-inner">@Resource.Other_Shape</div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="clearfix">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.SIZE_CARATS</h5>
                                    <div class="dyd-shape">

                                        <div class="diamondsize form-inline">
                                            <div class="radio">
                                                <label class="radio-inline"><input type="radio" id="quickSize" name="Size" value="QuickSize" checked>@Resource.Quick_Size</label>

                                                @*<label class="radio-inline"><input type="radio" id="singleSize" name="Size" value="SingleSize">Single Size</label>*@
                                            <label class="radio-inline"><input type="radio" id="multiSize" name="Size" value="MultipleSize">@Resource.Multiple_size</label>
                                            </div>
                                        </div>

                                        <form class="form-inline" id="frmSize">
                                            <div class="form-group">
                                                <input type="text" name="Sizefrom" maxlength="5" class="form-control" placeholder="From" value="" id="txtSizefrom">
                                            </div>
                                            <div class="form-group">
                                                <input type="text" name="Sizeto" maxlength="5" class="form-control" placeholder="To" value="" id="txtSizeto" compareTo="#txtSizefrom">
                                            </div>
                                            <div class="form-group">
                                                <button id="btnAddSizes" class="btn btn-primary" style="display:none">@Resource.Add</button>
                                            </div>
                                            <label id="SizeMsg" value="" style="color:red;"></label>
                                            <label class="reset-btn" style="display:none">@Resource.Reset1</label>
                                        </form>

                                        <div id="divMultipleSizes" class="form-inline" style="display:none">

                                        </div>


                                    </div>
                                    @*Added BY Ankit 30jun2020*@
                                    <div id="FquickSize" style="margin-top: 17px;">
                                        <form class="dyd-shape F_CaratsSize">


                                            @{ int CS = 1; }
                                            @foreach (var CaratSize in Model.VMCaratsSize)
                                            {
                                                var CSval = "CaratSize" + CS.ToString();
                                                <input type="checkbox" name="CaratSize" class="design-checkbox" value="@CaratSize.CaratFrm-@CaratSize.CaratTo" id="@CSval" />
                                                <label for="@CSval">@CaratSize.CaratFrm-@CaratSize.CaratTo  </label>
                                                CS = CS + 1;
                                            }

                                        </form>

                                    </div>
                                    <input type='button' value='Less' class="reset-btn" id='FCaratSize' style="margin-left: 70%;">
                                    <label class="reset-btn" id='btnF_CaratSize'>@Resource.Reset1</label>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.Color</h5>
                                    <form class="dyd-shape F_Color">
                                        @foreach (var Color in Model.VMcolor)
                                        {
                                            <input type="checkbox" name="color" class="design-checkbox" value="@Color.colorID" id="@Color.color_ID" />
                                            <label for="@Color.color_ID">@Color.colorName</label>
                                        }


                                        <input type="checkbox" class="design-checkbox" id="chkfancy_colorshow">
                                        <label onclick="$(this).toggleClass('selected');$('#fancy-colorshow').toggleClass('show-div');">Fancy Color</label>
                                        <input type='button' value='Less' class="reset-btn" id='FColorMore'>
                                        <label class="reset-btn" id='btnF_Color'>@Resource.Reset1</label>

                                    </form>

                                </div>
                            </div>
                        </div>

                        <div class="row" id="fancy-colorshow" style="display:none">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.FANCY_COLOR</h5>
                                    <form class="dyd-shape F_FancyColor">
                                        @{ int fc = 1; }
                                        @foreach (var ColorFancy in Model.VMfancyColor)
                                        {
                                            var fcval = "fancy" + fc.ToString();
                                            <input type="checkbox" name="fancy" class="design-checkbox" value="@ColorFancy.fancyColorID" id="@fcval" />
                                            <label for="@fcval">@ColorFancy.fancyColor</label>
                                            fc = fc + 1;
                                        }
                                    <label class="reset-btn" id='btnF_FancyColor'>@Resource.Reset1</label>
                                    </form>
                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.Clarity</h5>
                                    <form class="dyd-shape F_Clarity">
                                        @{ int cty = 1; }
                                        @foreach (var Clarity in Model.VMclarity)
                                        {
                                            var ctyval = "clarity" + cty.ToString();
                                            <input type="checkbox" name="clarity" class="design-checkbox" value="@Clarity.clarityID" id="@ctyval" />
                                            <label for="@ctyval">@Clarity.clarityName</label>
                                            cty = cty + 1;
                                        }
                                        <input type='button' value='Less' class="reset-btn" id='FClarityMore'>

                                        <label class="reset-btn" id='btnF_Clarity'>@Resource.Reset1</label>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.CERTIFICATE</h5>
                                    <form class="dyd-shape F_Certificate">
                                        @*<div>*@
                                        @{ int j = 1; }
                                        @foreach (var Lab in Model.VMlab)
                                        {
                                            var idlab = "lab" + j.ToString();
                                            <input type="checkbox" name="lab" class="design-checkbox" value="@Lab.labID" id="@idlab" />
                                            <label for="@idlab">@Lab.labName</label>
                                            j = j + 1;
                                        }
                                        <input type='button' value='Less' class="reset-btn" id='FCertificateMore'>

                                        <label class="reset-btn" id='btnF_Certificate'>@Resource.Reset1</label>
                                        @*</div>*@
                                    </form>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.FLUORESCENCE</h5>
                                    <form class="dyd-shape F_Fluorescence">
                                        @{ int fl = 1; }
                                        @foreach (var Fluorescence in Model.VMfluorescence)
                                        {
                                            var fluorval = "fluor" + fl.ToString();
                                            <input type="checkbox" name="fluor" class="design-checkbox" value="@Fluorescence.fluorescenceID" id="@fluorval" />
                                            <label for="@fluorval">@Fluorescence.fluorescenceName</label>
                                            fl = fl + 1;
                                        }
                                    <label class="reset-btn" id='btnF_Fluorescence'>@Resource.Reset1</label>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix" style="border-bottom: 1px solid #4d80b1;">
                                    <h5 class="describe-title">@Resource.CUT_POLISH_SYMMETRY</h5>
                                    <form class="dyd-shape">
                                        <div class="row">
                                            <div class="col-md-2">
                                                <a href="#" class="sele-all" id="ancEx" data-select="true">@Resource.EX</a> |
                                                <a href="#" class="sele-all" id="ancVEx" data-select="true">@Resource.VG</a>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <div>
                                                    <label for="cut1" style="font-size: 15px;">@Resource.Cut</label>
                                                </div>
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="F_Cut">
                                                    <input type="checkbox" name="cut" class="design-checkbox exe" value="18" id="cut1">
                                                    <label for="cut1">@Resource.Excellent</label>

                                                    <input type="checkbox" name="cut" class="design-checkbox vGood" value="17" id="cut2">
                                                    <label for="cut2">@Resource.Very_Good</label>

                                                    <input type="checkbox" name="cut" class="design-checkbox" value="19" id="cut3">
                                                    <label for="cut3">@Resource.Good</label>

                                                    <input type="checkbox" name="cut" class="design-checkbox" value="22" id="cut4">
                                                    <label for="cut4">@Resource.Fair</label>

                                                    <input type="checkbox" name="cut" class="design-checkbox" value="25" id="cut5">
                                                    <label for="cut5">Poor</label>
                                                    <label class="reset-btn" id='btnF_Cut'>@Resource.Reset1</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <div>
                                                    <label for="cut1" style="font-size: 15px;">@Resource.Polished</label>
                                                </div>
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="F_Polished">
                                                    <input type="checkbox" name="Polished" class="design-checkbox exe" value="9" id="Polished1">
                                                    <label for="Polished1">@Resource.Excellent</label>

                                                    <input type="checkbox" name="Polished" class="design-checkbox vGood" value="8" id="Polished2">
                                                    <label for="Polished2">@Resource.Very_Good</label>

                                                    <input type="checkbox" name="Polished" class="design-checkbox" value="7" id="Polished3">
                                                    <label for="Polished3">@Resource.Good</label>

                                                    <input type="checkbox" name="Polished" class="design-checkbox" value="15" id="Polished4">
                                                    <label for="Polished4">@Resource.Fair</label>

                                                    <input type="checkbox" name="Polished" class="design-checkbox" value="19" id="Polished5">
                                                    <label for="Polished5">@Resource.Poor</label>
                                                    <label class="reset-btn" id='btnF_Polished'>@Resource.Reset1</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <div>
                                                    <label for="cut1" style="font-size: 15px;">@Resource.Symmetry</label>
                                                </div>
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="F_Symmerty">
                                                    <input type="checkbox" name="Symmerty" class="design-checkbox exe" value="8" id="Symmerty1">
                                                    <label for="Symmerty1">@Resource.Excellent</label>

                                                    <input type="checkbox" name="Symmerty" class="design-checkbox vGood" value="9" id="Symmerty2">
                                                    <label for="Symmerty2">@Resource.Very_Good</label>

                                                    <input type="checkbox" name="Symmerty" class="design-checkbox" value="10" id="Symmerty3">
                                                    <label for="Symmerty3">@Resource.Good</label>

                                                    <input type="checkbox" name="Symmerty" class="design-checkbox" value="16" id="Symmerty4">
                                                    <label for="Symmerty4">@Resource.Fair</label>

                                                    <input type="checkbox" name="Symmerty" class="design-checkbox" value="18" id="Symmerty5">
                                                    <label for="Symmerty5">@Resource.Poor</label>
                                                    <label class="reset-btn" id='btnF_Symmerty'>@Resource.Reset1</label>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.Hearts__Arrows</h5>
                                    <form class="dyd-shape F_Hearts">
                                        @{
                                            int msHA = 1;
                                            foreach (var HA in Model.VMha)
                                            {
                                                var idmsHA = "HA" + msHA.ToString();
                                                <input type="checkbox" name="HA" class="design-checkbox" value="@HA.HAid" id="@idmsHA" />
                                                <label for="@idmsHA">@HA.HA</label>
                                                msHA = msHA + 1;
                                            }
                                        }
                                    <label class="reset-btn" id='btnF_Hearts'>@Resource.Reset1</label>
                                    </form>

                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title"></h5>
                                    <form class="dyd-shape">
                                        <input type="checkbox" id="NOBGM" name="NOBGM" value="1" class="space_chk">
                                        <b>@Resource.NO_BGM</b>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <!-- SalesLocation begins-->
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.SALES_LOCATION</h5>
                                    <form class="dyd-shape F_SalesL">
                                        @foreach (var sl in Model.VMSalesLocation)
                                        {
                                            <input type="checkbox" name="SLocation" class="design-checkbox" value="@sl.salesLocationID" id="SalesLocation@(sl.salesLocationID)" />
                                            <label for="SalesLocation@(sl.salesLocationID)">@sl.locationName</label>
                                        }
                                    <label class="reset-btn" id='btnF_SalesL'>@Resource.Reset1</label>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!-- SalesLocation end-->

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="describe-line clearfix">
                                    <h5 class="describe-title">@Resource.Stone_Origin</h5>
                                    <form class="dyd-shape F_Origin">
                                        @foreach (var StoneOrigin in Model.VMStoneOrigin)
                                        {
                                            <input type="checkbox" name="Origin" class="design-checkbox" value="@StoneOrigin.originID" id="Origin@(StoneOrigin.originID)" />
                                            <label for="Origin@(StoneOrigin.originID)">@StoneOrigin.originName</label>
                                        }
                                    <label class="reset-btn" id='btnF_Origin'>@Resource.Reset1</label>

                                    </form>

                                </div>
                            </div>
                        </div>

                        <!-- KeyTosymbol begins-->
                        <div class="padding-10">
                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    <a data-toggle="collapse" href="#collapsekeytoSymbols" class="" aria-expanded="true"><div class="panel-heading">@Resource.Key_To_Symbols <span class="glyphicon glyphicon-plus pull-right"> </span></div></a>
                                </div>

                                <div id="collapsekeytoSymbols" class="panel-collapse collapse" aria-expanded="true" style="">
                                    <div class="panel-body">
                                        <div class="describe-line clearfix" style="border: 0 none;background: none;">
                                            <div class="radio text-center">
                                                <label class="radio-inline"><input type="radio" id="KTScontains" name="r1" value="" checked><b>@Resource.Contains</b></label>
                                                <label class="radio-inline"><input type="radio" id="KTSNotcontains" name="r1" value=""><b>@Resource.Does_not_Contains</b></label>
                                            </div>
                                            <form class="dyd-shape" style="margin: 15px 0 0;">
                                                <div class="key-symbcheckbox F_Keytosymbol">
                                                    @{
                                                        int KTS = 1;
                                                        foreach (var mstKeytosymbol in Model.VMkeyToSymbol)
                                                        {
                                                            var KTSval = "KTS" + KTS.ToString();
                                                            <span class="key-rep">
                                                                <input type="checkbox" name="KTS" class="design-checkbox" value="@mstKeytosymbol.keyToSymbolID" id="@KTSval" />
                                                                <label for="@KTSval">@mstKeytosymbol.KeyToSymbol</label>
                                                            </span>
                                                            KTS = KTS + 1;
                                                        }
                                                    }
                                                </div>
                                                <label class="reset-btn" id='btnF_Keytosymbol'>@Resource.Reset1</label>
                                            </form>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <!-- keytosymbol Ends-->
                    </div>

                            <div class="clearfix">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <div class="form-inline padding-top-10 padding-bottom-10 padding-left-10">
                                            <a id="advance" class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample">@Resource.ADVANCE_SEARCH <span class="glyphicon glyphicon-plus" aria-hidden="true"></span></a>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12">
            <form class="collapse padding-top-10" id="collapseExample">
                <div class="row">
                    <div class="col-lg-6 col-md-6">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div>
                                    <div class="row form-group">
                                        <div class="col-lg-2">@Resource.Girdle</div>
                                        <div class="col-lg-10">
                                            <div>
                                                @{ int grd = 1;
                                                    foreach (var Girdle in Model.VMgirdleNames)
                                                    {
                                                        var idgir = "girdle" + grd.ToString();
                                                        <input type="checkbox" name="girdle" class="design-checkbox" value="@Girdle.girdleNameID" id="@idgir" />
                                                        <label for="@idgir">@Girdle.girdleName</label>
                                                        grd = grd + 1;
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row form-group">
                                        <div class="col-lg-2">@Resource.Girdle_Per</div>
                                        <div class="col-lg-5">
                                            <input type="text" maxlength="3" class="form-control" name="GRDLPERFROM" id="txtgirlefrom" placeholder="from" />
                                        </div>
                                        <div class="col-lg-5">
                                            <input type="text" maxlength="3" class="form-control" name="GRDLPERTO" id="txtgirleto" placeholder="to" compareTo="#txtgirlefrom" />
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col-lg-2">@Resource.Table</div>
                                        <div class="col-lg-5">
                                            <input type="text" maxlength="6" class="form-control" name="TBLPERFROM" id="txttablefrom" placeholder="from" />
