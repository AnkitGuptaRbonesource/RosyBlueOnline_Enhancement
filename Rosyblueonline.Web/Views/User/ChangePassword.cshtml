@{
    ViewBag.Title = "ChangePassword";
}
@section header{
}
<div class="content-wrapper">
    <div id="uiPanel1"></div>
    <section class="content">
        <div class="row">
            <div class="col-lg-12">
                <form id="frmResetPassword">
                    <div class="col-lg-6 col-md-6 col-md-offset-3">
                        <div class="panel panel-primary proshad">
                            <div class="panel-heading">
                                Change Password
                            </div>
                            <div class="panel-body">
                                <div class="">
                                    <div class="customers-info">
                                        <div class="">
                                            <div class="form-group clearfix">
                                                <div class="col-lg-3"><label for="txtOldPassword" class="control-label">*Old Password</label></div>
                                                <div class="col-lg-9">
                                                    <input class="form-control" id="txtOldPassword" name="password" type="password">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <div class="col-lg-3"><label for="txtNewPassword" class="control-label">*New Password</label></div>
                                                <div class="col-lg-9">
                                                    <input class="form-control" id="txtNewPassword" name="password" type="password">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <div class="col-lg-3"><label for="txtConfirmNewPassword" class="control-label">*Confirm Password</label></div>
                                                <div class="col-lg-9">
                                                    <input class="form-control" id="txtConfirmNewPassword" name="confirmPassword" type="password">
                                                </div>
                                            </div>
                                            <div class="form-group clearfix">
                                                <div class="col-lg-6"><input type="button" class="btn btn-primary pull-right" id="btnCancel" value="Cancel"  onclick="location.href='@Url.Action("Admin", "Dashboard" )'" ></div>
                                                 

                                                <div class="col-lg-6"><input type="submit" class="btn btn-primary pull-right" id="btnSubmit" value="Submit"></div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>


            </div>
        </div>
    </section>
</div>

@section script{
    <script src="~/Scripts/custom/Controller/CtrlResetPassword.js"></script>
}
